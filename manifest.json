{
  "manifest_version": 2,

  "name": "Daily Reading and Gratitudes",
  "description": "This extension allows the user to see daily gospel and post gratitudes anonymously",
  "version": "0.0.1.1",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "content_scripts": [ {
    "js": [ "./js/vendor/jquery-3.2.1.min.js",
      "./newtab.js",
      "./js/vendor/bootstrap.min.js",
      "./js/vendor/socket.io.min.js",
      "./js/vendor/twemoji.min.js",
      "./js/gratitude.js",
      "./js/main.js"
    ],
    "css": ["./css/vendor/bootstrap.min.css",
    "./css/vendor/google-font.css",
      "./css/main.css"],
    "matches": ["http://*/*", "https://*/*"]
  }]
}